## Vite client build-time vars (required for docker:full)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here
VITE_API_BASE_URL=https://api.example.com/api
VITE_AI_WS_URL=wss://ai.example.com/id/ws
VITE_GOOGLE_MAPS_API_KEY=your-google-maps-api-key-here
VITE_GOOGLE_MAP_ID=your-google-map-id-here

## Docker port overrides (host bindings)
CLIENT_PORT=15173
SERVER_PORT=13000
AI_PORT=18000

## Docker resource limits
CLIENT_MEM_LIMIT=256m
CLIENT_CPUS=0.50
SERVER_MEM_LIMIT=768m
SERVER_CPUS=1.5
AI_MEM_LIMIT=1024m
AI_CPUS=2.0
AI_GPU_DEVICE=0
POSTGRES_MEM_LIMIT=512m
POSTGRES_CPUS=0.50
CLOUDFLARED_MEM_LIMIT=128m
CLOUDFLARED_CPUS=0.25

## Postgres password used by compose
POSTGRES_PASSWORD=postgres
