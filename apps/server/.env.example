PORT=3000
NODE_ENV=development

# Database (Local Docker default)
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/eatable_db?schema=public"
# Prisma uses DIRECT_URL for certain operations (e.g., migrations) when defined.
DIRECT_URL="postgresql://postgres:postgres@localhost:5432/eatable_db?schema=public"

# Supabase (Auth + Storage)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-role-key-here

# AI Service (FastAPI)
AI_SERVICE_URL=http://localhost:8000

# Gemini (server agent + validation)
GEMINI_API_KEY=your-gemini-api-key-here
# Optional override (defaults to gemini-2.5-flash)
GEMINI_MODEL=gemini-3-flash-preview

# NETS Sandbox (required for NETS QR payment flows)
NETS_BASE_URL=https://sandbox.nets.openapipaas.com/api/v1
NETS_SANDBOX_API_KEY=your-nets-sandbox-api-key
NETS_SANDBOX_PROJECT_ID=your-nets-sandbox-project-id


# OpenAI (optional; used by tagging utilities)
OPENAI_API_KEY=your-openai-api-key
OPENAI_API_URL=https://api.openai.com/v1/responses

